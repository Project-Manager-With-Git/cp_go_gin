package main

import (
	"os"

	"{{ app_name }}/serv"

	log "github.com/Golang-Tools/loggerhelper"
	s "github.com/Golang-Tools/schema-entry-go"
)

// @title {{ app_name }}
// @version {{ version }}
// @description 测试

// @contact.name {{ author }}
// @contact.email {{ author_email }}

// @license.name Apache 2.0
// @license.url http://www.apache.org/licenses/LICENSE-2.0.html

// @host localhost:5000
// @BasePath /v{{ version | replace(".", "_") }}
func main() {
	serv, err := s.New(&s.EntryPointMeta{Name: "{{ app_name }}", Usage: "{{ app_name }} [options]"}, &serv.ServNode)
	if err != nil {
		log.Error("create entrypoint get error", log.Dict{"err": err})
		os.Exit(2)
	}
	serv.Parse(os.Args)
}
