package apis

import (
	"{{ app_name }}/apis/{{ apinamespace }}namespace"

	"github.com/gin-gonic/gin"
)

// Init 初始化路由
func Init(app *gin.Engine) *gin.Engine {

	// 注册api路由
	// 用户信息
	app = RegistPing(app)
	user := app.Group("/v{{ version | replace(".", "_") }}/api/user")
	{{ apinamespace }}namespace.Init(user)
	return app
}
