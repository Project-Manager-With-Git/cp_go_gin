package downloads

import (
	"{{ app_name }}/downloads/{{ downloadnamespace }}namespace"

	"github.com/gin-gonic/gin"
)

// Init 初始化路由
func Init(app *gin.Engine) *gin.Engine {

	// 注册api路由
	// 用户信息
	table := app.Group("/v{{ version | replace(".", "_") }}/download/table")
	{{ downloadnamespace }}namespace.Init(table)
	return app
}
